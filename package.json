{
  "name": "node-google-translator",
  "version": "1.0.1",
  "description": "A free and unlimited API for Google Translate for Node.js",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js",
      "types": "./dist/types/index.d.ts"
    }
  },
  "scripts": {
    "build": "npm run clean && npm run build:cjs && npm run build:esm && npm run build:types && node scripts/postbuild.js",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build:esm": "tsc -p tsconfig.esm.json",
    "build:types": "tsc -p tsconfig.types.json",
    "clean": "rimraf dist",
    "test": "node test/test.js",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "google-translate",
    "translator",
    "api",
    "node",
    "google",
    "translate",
    "language"
  ],
  "files": [
    "dist"
  ],
  "author": "1ly4s0",
  "license": "MIT",
  "dependencies": {
    "http-errors": "^2.0.0",
    "node-fetch": "^2.6.7"
  },
  "devDependencies": {
    "@types/http-errors": "^2.0.1",
    "@types/node": "^18.0.0",
    "@types/node-fetch": "^2.6.2",
    "rimraf": "^5.0.0",
    "typescript": "^5.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/1ly4s0/node-google-translator.git"
  },
  "bugs": {
    "url": "https://github.com/1ly4s0/node-google-translator/issues"
  },
  "homepage": "https://github.com/1ly4s0/node-google-translator#readme"
}
